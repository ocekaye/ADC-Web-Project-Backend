{
  "name": "Account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp" : {
        "required" : false
      }
  },
  "properties": {
    "access": {
      "type": "number",
      "required": true
    },
    "picture": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "steam_url": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
